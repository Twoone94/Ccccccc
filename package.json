{
  "name": "bananago-restart",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "main": "electron/main.mjs",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 5174",
    "desktop:dev": "cross-env DEV_SERVER_PORT=5174 concurrently \"vite --host 0.0.0.0 --port 5174\" \"wait-on http://localhost:5174 && electron .\"",
    "dev:5175": "vite --host 0.0.0.0 --port 5175",
    "desktop:dev:5175": "cross-env DEV_SERVER_PORT=5175 concurrently \"vite --host 0.0.0.0 --port 5175\" \"wait-on http://localhost:5175 && electron .\"",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
